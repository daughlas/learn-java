<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--
    namespace 命名空间，
    通常情况 = 当前操作实体类的全名称
-->
<mapper namespace="tech.lvjiawen.learnmybatis.entity.Users">
    <select id="usersList" resultType="tech.lvjiawen.learnmybatis.entity.Users">
        select * from users
    </select>
</mapper>